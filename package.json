{
  "name": "browserifytest",
  "version": "1.0.0",
  "description": "vue learn",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o public/build.js",
