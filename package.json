  "devDependencies": {
    "babel-core": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "browserify-hmr": "^0.3.5",
    "engine.io": "^2.0.1",
    "engine.io-client": "^2.0.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.1",
    "options": "^0.0.6",
    "parseuri": "^0.0.5",
    "uglify-js": "^2.7.5",
    "ultron": "^1.1.0",
