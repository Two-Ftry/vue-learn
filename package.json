{
  "name": "browserifytest",
  "version": "1.0.0",
  "description": "vue learn",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o public/build.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "ecstatic": "ecstatic public -p 8000",
    "dev": "npm-run-all --parallel watchify serve",
    "dev2": "npm-run-all --parallel watchify ecstatic",
    "build": "cross-env NODE_DEV=production browserify -g envify -t vueify -e src/main.js | uglifyjs > public/build.js",
    "start": "node ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Two-Ftry/vue-learn.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Two-Ftry/vue-learn/issues"
  },
  "homepage": "https://github.com/Two-Ftry/vue-learn#readme",
  "devDependencies": {
    "babel-core": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.4",
    "ecstatic": "^2.1.0",
    "engine.io": "^2.0.1",
    "engine.io-client": "^2.0.1",
    "envify": "^4.0.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.1",
    "options": "^0.0.6",
    "parseuri": "^0.0.5",
    "uglify-js": "^2.7.5",
    "ultron": "^1.1.0",
    "vueify": "^9.4.0",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "dependencies": {
    "vue": "^2.1.10",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  }
}
